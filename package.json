{
  "name": "swipe-bottom-sheet",
  "version": "0.2.14",
  "devDependencies": {
    "@angular/cdk": "^12.2.13",
    "@angular/cli": "~12.2.14",
    "@angular/common": "~12.2.15",
    "@angular/compiler": "~12.2.15",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/core": "~12.2.15",
    "@angular/language-service": "~12.2.15",
    "@angular/platform-browser": "^12.2.15",
    "release-it": "^14.11.6",
    "rxjs": "^6.6.7",
    "typescript": "^4.3.5",
    "zone.js": "^0.11.4"
  },
  "description": "[Demo](https://joeflateau.github.io/typescript-bottom-sheet/)",
  "main": "sheet.js",
  "scripts": {
    "deploy-demo": "(cd ./angular-demo; npm run ngh)",
